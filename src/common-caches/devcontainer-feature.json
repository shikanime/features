{
  "name": "Common Caches",
  "id": "common-caches",
  "version": "3.0.0",
  "description": "Setup common caches for various package managers.",
  "installsAfter": ["ghcr.io/devcontainers/features/common-utils"],
  "mounts": [
    {
      "source": "nix",
      "target": "/nix",
      "type": "volume"
    },
    {
      "source": "pip-cache",
      "target": "/home/vscode/.cache/pip",
      "type": "volume"
    },
    {
      "source": "yarn-cache",
      "target": "/home/vscode/.cache/yarn",
      "type": "volume"
    },
    {
      "source": "nix-cache",
      "target": "/home/vscode/.cache/nix",
      "type": "volume"
    },
    {
      "source": "huggingface-cache",
      "target": "/home/vscode/.cache/huggingface",
      "type": "volume"
    },
    {
      "source": "npm-cache",
      "target": "/home/vscode/.npm",
      "type": "volume"
    },
    {
      "source": "pdm-cache",
      "target": "/home/vscode/.pdm",
      "type": "volume"
    },
    {
      "source": "mix-cache",
      "target": "/home/vscode/.mix",
      "type": "volume"
    },
    {
      "source": "cargo-cache",
      "target": "/home/vscode/.cargo",
      "type": "volume"
    }
  ]
}

{
  "name": "Google Cloud SDK",
  "id": "google-cloud-sdk",
  "version": "1.2.0",
  "description": "Google Cloud SDK",
  "customizations": {
    "vscode": {
      "extensions": [
        "googlecloudtools.cloudcode"
      ]
    }
  },
  "containerEnv": {
    "PATH": "/usr/local/google-cloud-sdk/bin:${PATH}"
  },
  "installsAfter": [
    "ghcr.io/devcontainers/features/common-utils"
  ]
}
